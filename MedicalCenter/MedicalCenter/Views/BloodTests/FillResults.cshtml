<div class="d-flex">

    <partial name="_SideNavBarDoctors" />

    <form class="mx-auto half-width" asp-controller="Patient" asp-action="SendTest" asp-route-ID="@ViewData["TestId"]" method="post">
        <div class="form-group">
            @foreach (var p in this.Model)
            {
                <p>@p.Name</p>
                <input type="number" step="0.01" class="form-control" id="@p.Id" name="parameter">
            }
        </div>
        <div class="button-holder d-flex justify-content-center">
            <button type="submit" class="btn btn-success">Send</button>
        </div>

    </form>
</div>
